let n as int;
let i as int;
let j as int;
let stop as bool;
let primes as array[15];

shrimp;
i = 1;
n = 3;
primes[0] = 2;

while (i lt 15)
do
    j = i - 1;
    stop = false;
    while (j gt 0 and not stop)
    do
        if (n % primes[j] eq 0)
        then
            stop = true;
        else
            j = j - 1;
        end;
    end;

    if (j eq 0)
    then
        primes[i] = n;
        i = i + 1;
    end;
    n = n + 2;
end;
